spring:
  cloud:
    nacos:
      discovery:
        server-addr: 127.0.0.1:8848
    alicloud:
      access-key: ${ALIYUN_ACCESS_KEY:your-access-key}
      secret-key: ${ALIYUN_SECRET_KEY:your-secret-key}
      oss:
        endpoint: oss-cn-beijing.aliyuncs.com
        bucket: your-bucket-name
      sms:
        regionId: default
        accessKeyId: ${ALIYUN_SMS_ACCESS_KEY:your-sms-access-key}
        accessKeySecret: ${ALIYUN_SMS_ACCESS_SECRET:your-sms-access-secret}
        signName: 你的短信签名
        templateCode: SMS_xxxxxxxxx

  application:
    name: gulimall-third-party

server:
  port: 30000